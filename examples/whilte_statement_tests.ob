MODULE Figures;
BEGIN
    counter := 0;
    i := 0;
    WHILE i < 100
    DO
        counter := counter + 1;
        i := i + 1;
    END;

    counter := 0;
    i := 0;
    WHILE i < 10
    DO
        counter := counter + 1;
        i := i + 1;
    ELSIF i < 50
    DO
        counter := counter + 2;
        i := i + 2;
    ELSIF i < 100
    DO
        counter := counter + 3;
        i := i + 3;
    END;

END Figures.
